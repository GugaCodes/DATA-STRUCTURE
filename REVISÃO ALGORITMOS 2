#include <stdio.h>
#include <stdlib.h>

void insereValor(int valor, int numeros[], int* qtd){

	//valor = 0;
	numeros[*qtd] = valor;
	(*qtd)++;

}

void bubble_sort(int array[], int n) {
	for (int i = 0; i < n - 1; i++) {
		for (int j = 0; j < n - i - 1; j++) {
			if (array[j] > array[j + 1]){
				int troca;
				troca = array[j];
				array[j] = array[j + 1];
				array[j + 1] = troca;

			}
		}
	}
	
}

int main(){

	int vetor[5];
	int qtd = 0;
	
	insereValor(4, vetor, &qtd);
	insereValor(69, vetor, &qtd);
	insereValor(16, vetor, &qtd);
	insereValor(20, vetor, &qtd);
	insereValor(50, vetor, &qtd);

	/*for (int j = 0; j < 5; j++) {
		printf("%d\n",vetor[j]);
	}*/

	bubble_sort(vetor, qtd);
	

	for (int k = 0; k < 5; k++) {
		printf("%d\n", vetor[k]);
	}

	//printf("%d", vetor[4]);


	
}

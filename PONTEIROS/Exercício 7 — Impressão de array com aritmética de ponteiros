#include <stdio.h>

void imprime_array(const int *v, int n){
    for(int i = 0; i < n;i++){
        printf("%d |", *(v+i));
    }
    
}
int main()
{
    int array[6] = {0,69,21,63,24,8};
    int *p = array;
    imprime_array(p,6);
    
    
    
}
